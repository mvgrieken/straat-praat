{
  "name": "straat-praat",
  "version": "1.0.0",
  "description": "Een app voor ouders om jongerenslang te leren",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "export:web": "expo export --platform web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:web": "node scripts/build-web.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=e2e",
    "test:integration": "jest --testPathPattern=__tests__/services",
    "test:security": "jest --testPathPattern=__tests__/services --testNamePattern='(MFA|Security|Auth)'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "doctor": "expo doctor",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.51.23",
    "ajv": "^8.17.1",
    "expo": "~51.0.31",
    "expo-av": "~14.0.7",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.9",
    "expo-image": "~1.13.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.16",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-speech": "~12.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "is-number": "^7.0.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10",
    "zod": "^3.25.76",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/cli": "~0.18.25",
    "@expo/config-plugins": "~8.0.8",
    "@expo/metro-runtime": "~3.2.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^16.1.5",
    "node-fetch": "^2.7.0",
    "prettier": "^3.2.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.3.3",
    "url-parse": "^1.5.10"
  },
  "keywords": [
    "react-native",
    "expo",
    "straat-praat",
    "slang",
    "education"
  ],
  "author": "Martijn van Grieken",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
